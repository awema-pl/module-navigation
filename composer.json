{
    "name": "awema-pl/module-navigation",
    "description": "Navigation package for Awema layouts",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Karol Golec",
            "email": "karolgolec10@gmail.com",
            "homepage": "https://github.pl/awema-pl/module-navigation",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "karolgolec10@gmail.com"
    },
    "homepage": "https://www.awema.pl",
    "keywords": [
        "Laravel",
        "Navigation",
        "AwemaIO"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7|~8|~9",
        "awema-pl/module-localization-helper": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "AwemaPL\\Navigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AwemaPL\\Navigation\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwemaPL\\Navigation\\NavigationServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Localization-helper",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}